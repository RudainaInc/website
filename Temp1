<script type=text/javascript>
var delay="10"; 
var count='0';
var Texts=new Array();
Texts[0]="Our New Website and App have Officially Launched!";
Texts[1]="Something else Happened";
Texts[2]="Rudaina kicks Abortions ass";
function changeText(){
document.getElementById('textRange').innerHTML=Texts[count];
count++;
if(count==Texts.length){count='0';}
setTimeout("changeText()",5000);
}

</script>


<script type="text/javascript">

var arr = [];

arr[0]= new Image();
arr[0].src = "https://wallpaperbrowse.com/media/images/soap-bubble-1958650_960_720.jpg";

arr[1]= new Image();
arr[1].src = "https://images.pexels.com/photos/248797/pexels-photo-248797.jpeg?auto=compress&cs=tinysrgb&h=350";

arr[2]= new Image();
arr[2].src = "https://images.pexels.com/photos/34950/pexels-photo.jpg?auto=compress&cs=tinysrgb&h=350";

var i=0;

function slide(){
 document.getElementById("image1").src= arr[i].src;
 i++;
 if(i==arr.length){
  i=0;
 }
 setTimeout(function(){ slide(); },5000);
}

	</script>

